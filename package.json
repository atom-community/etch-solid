{
  "name": "etch-solid",
  "main": "dist/index.js",
  "version": "0.1.0",
  "author": "Amin Yahyaabadi",
  "description": "fast DOM library - etch with solid backend",
  "keywords": [
    "etch",
    "solid",
    "solidjs",
    "dom",
    "view",
    "element",
    "custom-elements",
    "atom",
    "electron",
    "react",
    "vue"
  ],
  "repository": "https://github.com/atom-ide-community/etch-solid",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "npm run mocha",
    "mocha": "electron-mocha test --renderer --recursive --require @babel/register",
    "tdd": "electron-mocha test --renderer --recursive --interactive --require @babel/register",
    "clean": "shx rm -rf dist",
    "tsc": "tsc -p src/tsconfig.json || echo done",
    "dev": "npm run clean && cross-env NODE_ENV=development cross-env BABEL_KEEP_MODULES=true rollup -c -w",
    "build": "npm run clean && cross-env NODE_ENV=production cross-env BABEL_KEEP_MODULES=true rollup -c ",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u -x coffeescript",
    "prepare": "npm run build"
  },
  "dependencies": {
    "babel-preset-solid": "1.8.0",
    "solid-js": "1.8.1",
    "solid-styled-jsx": "0.27.1",
    "styled-jsx": "5.1.2"
  },
  "devDependencies": {
    "@types/atom": "1.40.5",
    "@types/node": "^14.14.14",
    "typescript": "^4.1.3",
    "tslib": "^2.0.3",
    "coffeescript": "^1.12.7",
    "prettier": "^2.2.1",
    "eslint": "7.16.0",
    "eslint-config-atomic": "^1.5.1",
    "rollup": "2.35.1",
    "rollup-plugin-atomic": "^1.6.4",
    "shx": "0.3.3",
    "cross-env": "7.0.3",
    "npm-check-updates": "10.2.5",
    "build-commit": "0.1.1",
    "@babel/core": "^7.12.10",
    "@babel/cli": "^7.12.10",
    "babel-preset-atomic": "^3.0.1",
    "@babel/register": "^7.12.10",
    "chai": "^4.2.0",
    "electron": "^11.1.0",
    "electron-mocha": "^10.0.0",
    "estraverse-fb": "^1.3.2",
    "mocha": "^8.2.1",
    "random-seed": "^0.3.0"
  }
}
