{
  "name": "etch-solid",
  "main": "dist/main.js",
  "version": "0.1.0",
  "author": "Amin Yahyaabadi",
  "description": "fast DOM library - etch with solid backend",
  "keywords": [
    "etch",
    "solid",
    "solidjs",
    "dom",
    "view",
    "element",
    "custom-elements",
    "atom",
    "electron",
    "react",
    "vue"
  ],
  "repository": "https://github.com/atom-ide-community/etch-solid",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "npm run mocha",
    "mocha": "electron-mocha --renderer --recursive ./test/helpers/register-babel test",
    "tdd": "node_modules/.bin/electron-mocha --renderer --interactive --recursive ./test/helpers/register-babel  test",
    "clean": "shx rm -rf dist",
    "tsc": "tsc -p src/tsconfig.json || echo done",
    "dev": "npm run clean && cross-env NODE_ENV=development cross-env BABEL_KEEP_MODULES=true rollup -c -w",
    "build": "npm run clean && cross-env NODE_ENV=production cross-env BABEL_KEEP_MODULES=true rollup -c ",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u -x coffeescript",
    "prepare": "npm run build"
  },
  "dependencies": {
    "babel-preset-solid": "0.18.14",
    "solid-js": "0.18.14",
    "solid-styled-jsx": "0.18.14",
    "styled-jsx": "3.3.0"
  },
  "devDependencies": {
    "@types/atom": "1.40.4",
    "@types/node": "^14.6.0",
    "typescript": "^4.0.2",
    "tslib": "^2.0.1",
    "coffeescript": "^1.12.7",
    "prettier": "^2.0.5",
    "eslint": "7.7.0",
    "eslint-config-atomic": "^1.5.0",
    "rollup": "2.26.5",
    "rollup-plugin-atomic": "^1.2.0",
    "shx": "0.3.2",
    "cross-env": "7.0.2",
    "npm-check-updates": "7.1.1",
    "build-commit": "0.1.1",
    "chai": "^4.2.0",
    "electron": "^9.2.1",
    "electron-mocha": "9.1.0",
    "estraverse-fb": "^1.3.2",
    "mocha": "^3.1.2",
    "random-seed": "^0.3.0"
  }
}
